{
  "key": "causelist",
  "DataConfigs": {
    "serviceName": "rainmaker-common",
    "version": "1.0.0",
    "baseKeyPath": "$.CauseList",
    "entityIdPath": "$.*.caseId",
    "isCommonTableBorderRequired": true,
    "mappings": [
      {
        "topic": "common-pdf-generation-3",
        "mappings": [
          {
            "direct": [
              {
                "variable": "logoImage",
                "url": "https://raw.githubusercontent.com/Ajith-beehyv/birth-registration/main/kerala_hc.png",
                "type": "image"
              },
              {
                "variable": "causeLists",
                "value": {
                  "path": "$.*"
                },
                "type": "index",
                "format": " var i = 0; for (i=0;i< type.length;i++) {type[i]['index'] = i + 1;} return type; "
              }
            ]
          }
        ]
      }
    ]
  }
}

