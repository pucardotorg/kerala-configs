{
  "key": "warrants",
  "DataConfigs": {
    "serviceName": "rainmaker-common",
    "version": "1.0.0",
    "baseKeyPath": "$.TaskSummon",
    "entityIdPath": "$.SummonDetails.summonId",
    "isCommonTableBorderRequired": true,
    "mappings": [
      {
        "topic": "common-pdf-generation-3",
        "mappings": [
          {
            "direct": [
              {
                "variable": "respondentName",
                "value": {
                  "path": "$.respondentDetails.name",
                  "defaultValue": "NA"
                }
              },
              {
                "variable": "respondentAddress",
                "value": {
                  "path": "$.respondentDetails.address",
                  "defaultValue": "NA"
                }
              },
              {
                "variable": "caseCharge",
                "value": {
                  "path": "$.caseDetails.caseCharge",
                  "defaultValue": "NA"
                }
              },
              {
                "variable": "judgeName",
                "value": {
                  "path": "$.caseDetails.judgeName",
                  "defaultValue": "NA"
                }
              },
              {
                "variable": "courtName",
                "value": {
                  "path": "$.caseDetails.courtName",
                  "defaultValue": "NA"
                }
              },
              {
                "variable": "hearingDate",
                "value": {
                  "path": "$.caseDetails.hearingDate",
                  "defaultValue": "NA"
                }
              },
              {
                "variable": "hearingDay",
                "value": {
                  "path": "$.caseDetails.hearingDate",
                  "transform": "new Date(value).getDate()",
                  "defaultValue": "NA"
                }
              },
              {
                "variable": "hearingMonth",
                "value": {
                  "path": "$.caseDetails.hearingDate",
                  "transform": "new Date(value).toLocaleString('default', { month: 'long' })",
                  "defaultValue": "NA"
                }
              },
              {
                "variable": "issueDate",
                "value": {
                  "path": "$.summonDetails.issueDate",
                  "defaultValue": "NA"
                }
              },
              {
                "variable": "executorName",
                "value": {
                  "path": "$.summonDetails.executorName",
                  "defaultValue": "NA"
                }
              },
              {
                "variable": "bailAmount",
                "value": {
                  "path": "$.summonDetails.bailAmount",
                  "defaultValue": "NA"
                }
              },
              {
                "variable": "singleSuretyAmount",
                "value": {
                  "path": "$.summonDetails.singleSuretyAmount",
                  "defaultValue": "NA"
                }
              }
            ]
          }
        ]
      }
    ]
  }
}

