{
  "key": "case",
  "DataConfigs": {
    "serviceName": "pdf-service",
    "version": "1.0.0",
    "baseKeyPath": "$.caseDetails",
    "entityIdPath": "$.id",
    "isCommonTableBorderRequired": true,
    "mappings": [
      {
        "mappings": [
          {
            "direct": [
              {
                "variable": "courtName",
                "value":{
                  "path":"$.courtName"
                },
                "default": "District Court of Kollam"
              },
              {
                "variable": "caseNumber",
                "value":{
                  "path":"$.caseNumber"
                },
                "default": ""
