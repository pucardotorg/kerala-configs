                },
                "default": ""
              },
              {
                "variable": "sectionNumber",
                "value":{
                  "path":"$.sectionNumber"
                },
                "default": "U/s.138 of N.I ACT"
              },
              {
                "variable": "complainants",
                "value":{
                  "path":"$.complainants.*"
                },
                "type": "index",
                "format": " var i = 0; for (i=0;i< type.length;i++) {type[i]['index'] = i + 1;} return type; "
              },
