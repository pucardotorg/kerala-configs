{
  "key": "table-of-contents",
  "DataConfigs": {
    "serviceName": "pdf-service",
    "version": "1.0.0",
    "baseKeyPath": "$.Data.*",
    "entityIdPath": "$.id",
    "isCommonTableBorderRequired": true,
    "mappings": [
      {
        "topic": "common-pdf-generation-3",
        "mappings": [
          {
            "direct": [
              {
                "variable": "caseName",
                "value": {
                  "path": "$.caseName"
                }
              },
              {
                "variable": "documentList",
                "value": {
                  "path": "$.documentList"
                },
                "type": "function",
                "format": " var i = 0; for (i=0;i< type.length;i++) {type[i]['index'] = i + 1;} return type;"
              }
            ]
          }
        ]
      }
    ]
  }
}
