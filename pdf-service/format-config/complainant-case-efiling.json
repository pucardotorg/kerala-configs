{
  "key": "complainant-case-efiling",
  "config": {
    "content": [
      {
        "table": {
          "widths": ["*"],
          "body": [
            [
              {
                "stack": [
                  {
                    "text": "{{courtName}}",
                    "style": "header"
                  },
                  {
                    "text": "Filing No. {{filingNumber}}",
                    "style": "subHeader"
                  },
                  {
                    "text": "Complainant(s):",
                    "style": "title"
                  },
                  "{{#complainantList}}",
                  [
                    {
                      "table": {
                        "widths": ["*"],
                        "body": [
                          [
                            {
                              "stack": [
                                {
                                  "text": "Complainant {{index}}:",
                                  "style": "subTitle"
                                },
                                "{{#ifIndividual}}",
                                {
                                  "text": [
                                    {
                                      "text": "{{complainantName}}",
                                      "bold": true
                                    },
                                    {
                                      "text": "aged {{complainantAge}}"
                                    }
                                  ],
                                  "style": "bodyText"
                                },
                                {
                                  "text": "{{complainantAddress}} (Mobile No. {{phoneNumber}}; Email. {{emailId}})",
                                  "style": "bodyText"
                                },
                                "{{/ifIndividual}}",
                                "{{^ifIndividual}}",
                                {
                                  "text": "{{institutionName}}, {{complainantAddress}}",
                                  "style": "bodyText"
                                },
                                {
                                  "text": "Authorized Signatory - {{nameOfAuthorizedSignatory}}, {{designationOfAuthorizedSignatory}}",
                                  "style": "bodyText"
                                },
                                "{{/ifIndividual}}",
                                "{{#isPartyInPerson}}",
                                {
                                  "text": "by Self",
                                  "style": "bodyText"
                                },
                                "{{/isPartyInPerson}}",
                                "{{^isPartyInPerson}}",
                                {
                                  "table": {
                                    "widths": ["3%", "97%"],
                                    "body": [
                                      "{{#advocateList}}",
                                      [
                                        {
                                          "text": "{{^index}}by {{/index}}",
                                          "style": "advocateText",
                                          "border": [false, false, false, false]
                                        },
                                        {
                                          "text": "{{advocateName}}, Bar ID - {{barId}}, Phone Number - {{advocatePhoneNumber}}",
                                          "style": "advocateText",
                                          "border": [false, false, false, false]
                                        }
                                      ],
                                      "{{/advocateList}}"
                                    ]
                                  }
                                },
                                "{{/isPartyInPerson}}"
                              ],
                              "margin": [0, 0, 0, 10]
                            }
                          ]
                        ]
                      },
                      "layout": "noBorders"
                    }
                  ],
                  "{{/complainantList}}",
                  {
                    "text": "Accused(s):",
                    "style": "title"
                  },
                  "{{#accusedList}}",
                  [
                    {
                      "table": {
                        "widths": ["*"],
                        "body": [
                          [
                            {
                              "stack": [
                                {
                                  "text": "Accused {{index}}:",
                                  "style": "subTitle"
                                },
                                "{{#ifAccusedIndividual}}",
                                {
                                  "text": "{{accusedName}}:",
                                  "style": "bodyText",
                                  "bold": true
                                },
                                {
                                  "text": "Approximate Age: {{accusedAge}}",
                                  "style": "bodyText"
                                },
                                {
                                  "text": "{{accusedAddress}} (Mobile No. {{accusedPhoneNumber}}; Email. {{accusedEmailId}})",
                                  "style": "bodyText"
                                },
                                "{{/ifAccusedIndividual}}",
                                "{{^ifAccusedIndividual}}",
                                {
                                  "text": "{{accusedInstitutionName}}, {{accusedAddress}}",
                                  "style": "bodyText"
                                },
                                {
                                  "text": "Authorized Signatory - {{nameOfAccusedAuthorizedSignatory}}, {{designationOfAccusedAuthorizedSignatory}}",
                                  "style": "bodyText"
                                },
                                "{{/ifAccusedIndividual}}"
                              ],
                              "margin": [0, 0, 0, 10]
                            }
                          ]
                        ]
                      },
                      "layout": "noBorders"
                    }
                  ],

                  "{{/accusedList}}",
                  {
                    "text": "COMPLAINT FILED UNDER SECTION 138 and 142 OF THE NEGOTIABLE INSTRUMENTS ACT",
                    "style": "complaintHeader",
                    "decoration": "underline"
                  },
                  {
                    "text": "The complainant most humbly submits as follows:",
                    "style": "complaintSubHeader"
                  },
                  {
                    "text": "{{complaint}}",
                    "style": "bodyText"
                  },
                  {
                    "text": "Prayer",
                    "style": "title",
                    "decoration": "underline"
                  },
                  {
                    "text": "{{prayer}}",
                    "style": "bodyText"
                  },
                  {
                    "text": "Verification",
                    "style": "title",
                    "decoration": "underline"
                  },
                  {
                    "text": "All the facts stated above are true and correct to the best of my knowledge, information and belief. The physical/electronic record of the digital documents etc. produced by me as appended below are in my lawful and proper custody and possession.",
                    "style": "bodyText"
                  },
                  {
                    "text": "Dated the {{dateOfFiling}}",
                    "style": "date"
                  },
                  {
                    "table": {
                      "widths": ["50%", "50%"],
                      "body": [
                        [
                          {
                            "text": "Complainant Signature(s)",
                            "alignment": "left",
                            "style": "signatureHeading"
                          },
                          {
                            "text": "Advocate Signature(s)",
                            "alignment": "right",
                            "style": "signatureHeading"
                          }
                        ],
                        "{{#complainantList}}",
                        [
                          {
                            "text": "Complainant {{index}} Signature",
                            "style": "signature",
                            "alignment": "left"
                          },
                          {
                            "text": "Advocate {{index}} Signature",
                            "style": "signature",
                            "alignment": "right",
                            "color": "white"
                          }
                        ],
                        "{{/complainantList}}"
                      ]
                    },
                    "layout": "noBorders"
                  },
                  {
                    "text": "(Signature of one advocate representing each complainant is needed. In case an advocate represents multiple complainants, they only need to sign once)",
                    "style": "bodyText",
                    "margin": [0, 10, 0, 3]
                  },
                  {
                    "text": "List of Documents:",
                    "style": "title",
                    "decoration": "underline"
                  },
                  {
                    "ol": [
                      "{{#documentList}}",
                      {
                        "text": "{{.}}",
                        "style": "bodyText"
                      },
                      "{{/documentList}}"
                    ],
                    "style": "list"
                  },
                  {
                    "text": "Witness schedule",
                    "style": "title",
                    "decoration": "underline"
                  },
                  {
                    "style": "table",
                    "table": {
                      "widths": [
                        "10%",
                        "15%",
                        "15%",
                        "15%",
                        "15%",
                        "15%",
                        "15%"
                      ],
                      "body": [
                        [
                          {
                            "text": "S. No.",
                            "style": "bodyText",
                            "bold": true,
                            "border": [true, true, true, true]
                          },
                          {
                            "text": "Name",
                            "style": "bodyText",
                            "bold": true,
                            "border": [true, true, true, true]
                          },
                          {
                            "text": "Occupation",
                            "style": "bodyText",
                            "bold": true,
                            "border": [true, true, true, true]
                          },
                          {
                            "text": "Phone Number",
                            "style": "bodyText",
                            "bold": true,
                            "border": [true, true, true, true]
                          },
                          {
                            "text": "E-Mail",
                            "style": "bodyText",
                            "bold": true,
                            "border": [true, true, true, true]
                          },
                          {
                            "text": "Address",
                            "style": "bodyText",
                            "bold": true,
                            "border": [true, true, true, true]
                          },
                          {
                            "text": "Additional Details",
                            "style": "bodyText",
                            "bold": true,
                            "border": [true, true, true, true]
                          }
                        ],
                        "{{#witnessScheduleList}}",
                        [
                          {
                            "text": "{{index}}",
                            "style": "bodyText",
                            "border": [true, true, true, true]
                          },
                          {
                            "text": "{{witnessName}}",
                            "style": "bodyText",
                            "border": [true, true, true, true]
                          },
                          {
                            "text": "{{witnessOccupation}}",
                            "style": "bodyText",
                            "border": [true, true, true, true]
                          },
                          {
                            "text": "{{witnessPhoneNumber}}",
                            "style": "bodyText",
                            "border": [true, true, true, true]
                          },
                          {
                            "text": "{{witnessEmail}}",
                            "style": "bodyText",
                            "border": [true, true, true, true]
                          },
                          {
                            "text": "{{witnessAddress}}",
                            "style": "bodyText",
                            "border": [true, true, true, true]
                          },
                          {
                            "text": "{{witnessAdditionalDetails}}",
                            "style": "bodyText",
                            "border": [true, true, true, true]
                          }
                        ],
                        "{{/witnessScheduleList}}"
                      ]
                    }
                  }
                ],
                "alignment": "left",
                "margin": [0, 0, 0, 20]
              }
            ]
          ]
        },
        "layout": "noBorders"
      }
    ],
    "styles": {
      "header": {
        "fontSize": 18,
        "bold": true,
        "alignment": "center",
        "margin": [0, 10, 0, 0]
      },
      "subHeader": {
        "fontSize": 16,
        "alignment": "center",
        "margin": [0, 10, 0, 0]
      },
      "title": {
        "fontSize": 16,
        "bold": true,
        "margin": [0, 30, 0, 5]
      },
      "subTitle": {
        "fontSize": 14,
        "bold": true,
        "margin": [0, 5, 0, 5]
      },
      "complaintHeader": {
        "fontSize": 18,
        "bold": true,
        "alignment": "center",
        "margin": [0, 30, 0, 20]
      },
      "complaintSubHeader": {
        "fontSize": 13,
        "margin": [0, 5, 0, 10]
      },
      "complaintSubTitle": {
        "fontSize": 15,
        "margin": [0, 5, 0, 5]
      },
      "bodyText": {
        "fontSize": 13,
        "margin": [0, 3, 0, 3]
      },
      "advocateText": {
        "fontSize": 13,
        "margin": [0, 0, 0, 0]
      },
      "date": {
        "fontSize": 13,
        "margin": [0, 20, 0, 3]
      },
      "signatureHeading": {
        "decoration": "underline",
        "fontSize": 13.5,
        "margin": [0, 20, 0, 3]
      },
      "signature": {
        "fontSize": 13,
        "margin": [0, 55, 0, 3]
      },
      "table": {
        "margin": [0, 15, 0, 0]
      }
    }
  }
}
