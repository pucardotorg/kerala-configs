            [
              {
                "stack": [
                  {
                    "text": "IN THE COURT OF {{courtName}}",
                    "style": "header"
                  },
                  {
                    "text": "{{judgeName}}",
                    "style": "subheader"
                  },
                  {
                    "text": "{{judgeDesignation}}",
                    "style": "subheader"
                  },
                  {
                    "text": "{{judgementDate}}",
                    "style": "subheader"
                  },
                  {
                    "text": "{{caseNumber}}",
                    "style": "subheader"
                  },
                  {
                    "table": {
                      "widths": [150, "*"],
                      "body": [
                        [
                          {
                            "text": "Complainant",
                            "style": "tableData"
                          },
                          {
                            "text": "{{complaintName}}, aged {{complaintAge}}, {{complaintAddress}}, {{complaintAdvocate}}",
                            "style": "tableData"
                          }
                        ],
                        [
                          {
                            "text": "Accused",
                            "style": "tableData"
                          },
                          {
                            "text": "{{respondantName}}, aged {{respondantAge}}, s/o {{fatherName}}, {{respondantAddress}}, {{respondantAdvocate}}",
                            "style": "tableData"
                          }
                        ],
                        [
                          {
                            "text": "Offence",
                            "style": "tableData"
                          },
                          {
                            "text": "{{sectionAct}}",
                            "style": "tableData"
                          }
                        ],
                        [
                          {
                            "text": "Plea",
