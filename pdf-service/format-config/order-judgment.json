                          },
                          {
                            "text": "{{defenceWitnessContent}}",
                            "style": "tableData"
                          }
                        ],
                        [
                          {
                            "text": "PW{{defenceWitnessIndex}}",
                            "style": "tableData"
                          },
                          {
                            "text": "{{defenceWitnessContent}}",
                            "style": "tableData"
                          }
                        ]
