                        [
                          {
                            "text": "Finding",
                            "style": "tableData"
                          },
                          {
                            "text": "{{finding}}",
                            "style": "tableData"
                          }
                        ],
                        [
                          {
                            "text": "Sentence/Order",
                            "style": "tableData"
                          },
                          {
                            "text": "{{sentence}}",
                            "style": "tableData"
                          }
                        ]
                      ],
                      "border": [true, true, true, true]
                    }
                  },
                  {
                    "text": "DESCRIPTION OF ACCUSED",
                    "style": "tableHeader"
                  },
